Proceso sin_titulo
	// ingresar 3 notas, calcular el promedio y mostrar el resultado
	definir n1,n2,n3,prom Como Entero;
	escribir "Digite primera nota: "; leer n1;
	escribir "Digite segunda nota: "; leer n2;
	escribir "Digite tercera nota: "; leer n3;
	prom<-(n1+n2+n3)/3;
	Escribir "El promedio obtenido es: ", prom;
FinProceso
